<div class="login-title">
  <h1 class="h1-login">login</h1>
</div>

<div class="box-container">

  <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      <%= f.submit "Log in" %>
    <% end %>
</div>